QMainWindow#MainWindow
{
    min-width: 68em;
    min-height: 42em;
}

QWidget
{
    font: 9pt "Ubuntu";
    outline: none;
}

QWidget:focus
{
    outline: none;
}

QWidget[Background="true"]
{
    background-color: #ffffff;
}

QWidget#widgetBase
{
    border: none;
    border-radius: 0.3em;
    background-color: transparent;
}

QWidget#widgetLeftBase
{
    background-color: #295e9f;
    border: none;
    border-radius: 0.3em;
    min-width: 12em;
    max-width: 12em;
    border-top-right-radius: 0px;
    border-bottom-right-radius: 0px;
}

QWidget#widgetContentBase
{
    background-color: #f0f0f0;
    border: none;
    border-right: 1px solid #d0d0d0;
    border-bottom: 1px solid #d0d0d0;
    border-left: 1px solid #d0d0d0;
    border-radius: 0.4em;
}

QWidget#widgetSettingTopBase
{
    background-color: #f0f0f0;
    border: none;
    border-right: 1px solid #d0d0d0;
    border-bottom: 1px solid #d0d0d0;
    border-left: 1px solid #d0d0d0;
    border-bottom-left-radius: 0.4em;
    border-bottom-right-radius: 0.4em;
}

QWidget#widgetTitle
{
    background-color: #fefefe;
    border: none;
    border-top: 1px solid #d0d0d0;
    border-right: 1px solid #d0d0d0;
    border-bottom: 1px solid #d0d0d0;
    min-height: 2.0em;
    border-top-right-radius: 0.3em;
}

QWidget#widgetSettingTitle
{
    background-color: #fefefe;
    border: 1px solid #d0d0d0;
    min-height: 2.0em;
    border-top-right-radius: 0.3em;
    border-top-left-radius: 0.3em;
}

QWidget#widgetContent,#widgetSearch
{
    background-color: #fefefe;
    border-radius: 0.3em;
}

QWidget#widgetClientType,#widgetSearchModeBase,#widgetTargetBase,#widgetSourceBase
{
    min-width: 9.6em;
    background-color: #fefefe;
    border: 1px solid #dcdfe6;
    border-radius: 0.3em;
}

QWidget#widgetSettingBase
{
    background-color: transparent;
}

QWidget#widgetViewBase
{
    border-radius: 0.3em;
}

/* QPushButton */
QPushButton[Ok="true"]
{
    min-width: 1.6em;
    min-height: 1.8em;
    border-radius: 0.2em;
    padding-left: 1em;
    padding-right: 1em;
    color: #fefefe;
    background-color: #409eff;
}

QPushButton[Ok="true"]:hover
{
    background-color: #66b1ff;
}

QPushButton[Ok="true"]:pressed
{
    background-color: #409eff;
}

QPushButton[Ok="true"]:!enabled
{
    background-color: #444444;
}

QPushButton[Cancel="true"]
{
    min-width: 1.6em;
    min-height: 1.8em;
    border-radius: 0.2em;
    border: 1px solid #dcdfe6;
    border-radius: 0.2em;
    padding-left: 1em;
    padding-right: 1em;
    color: #666666;
    background-color: #f3f4f5;
}

QPushButton[Cancel="true"]:hover
{
    background-color: #fefefe;
}

QPushButton[Cancel="true"]:pressed
{
    background-color: #f3f4f5;
}

QPushButton[Cancel="true"]:!enabled
{
    background-color: #444444;
}

QPushButton[Flat="true"]
{
    border: none;
    margin-right: 1em;
    color: #999999;
    background-color: #fefefe;
}

QPushButton[Flat="true"]:hover
{
    color: #fc9153;
}

QPushButton#btnSystemSet
{
    min-width: 1.6em;
    max-width: 1.6em;
    min-height: 1.6em;
    max-height: 1.6em;
    background-color: transparent;
    padding: 0.3em;
    border-radius: 0.3em;
    color: #fefefe;
    image: url(:/Resource/image/setting.svg);
}

QPushButton#btnSystemSet:hover
{
    background-color: #4b7ebe;
}

QPushButton#btnSystemSet:pressed
{
    background-color: #3b6eae;
}

QPushButton[SmallToolButton]
{
    min-width: 1.2em;
    max-width: 1.2em;
    min-height: 1.2em;
    max-height: 1.2em;
    background-color: transparent;
    padding: 0.3em;
    border-radius: 0.2em;
    color: #fefefe;
}

QPushButton[SmallToolButton]:hover
{
    background-color: #dcdfe6;
}

QPushButton[SmallToolButton]:pressed
{
    background-color: #d0d0d0;
}

/* QLineEdit */
QLineEdit
{
    border-radius: 0.1em;
    color: #666666;
}

QLineEdit#tbRootDir,#tbSourceDir
{
    min-height: 1.8em;
    border: none;
    border-left: 1px solid #dcdfe6;
    padding-left: 0.64em;
    margin-right: 1em;
    border-radius: 0.3em;
    border-top-left-radius: 0em;
    border-bottom-left-radius: 0em;
    color: #3f8f54;
}

/* QListView */
QListView
{
    border: 1px solid #dcdfe6;
    border-radius: 0.3em;
    padding: 0.3em;
}

QListView::item
{
    background-color: #fefefe;
    min-height: 1.2em;
    color: #666666;
    padding-left: 0.3em;
    border-radius: 0.3em;
}

QListView#lvTargetPath::item
{
    min-height: 3.2em;
}

QListView::item:hover
{
    background-color: #55dcdfe6;
}

QListView::item:selected
{
    color: #409eff;
    background-color: #dcdfe6;
}

/* QComboBox */
QComboBox
{
    min-width: 5.0em;
    min-height: 1.8em;
    border: 1px solid #dcdfe6;
    border-radius: 0.3em;
    border-top-left-radius: 0em;
    border-bottom-left-radius: 0em;
    padding-left: 0.3em;
    color: #666666;
}

QComboBox QAbstractItemView
{
    outline: none;
    padding-top: 0.3em;
    border: 1px solid #d0d0d0;
    border-radius: 0.3em;
    padding-bottom: 0.3em;
    background-color: #fefefe;
}

QComboBox::drop-down
{
    padding-right: 1em;
    border: none;
    min-width: 1em;
    min-height: 1em;
}

QComboBox::down-arrow
{
    border-image: url(:/Resource/image/combobox_down.png);
    min-width: 1em;
    min-height: 1em;
}

QComboBox QAbstractItemView::item
{
    padding-top: 0.3em;
    padding-bottom: 0.3em;
    border-radius: 0.2em;
    padding-left: 0.5em;
}

QComboBox QAbstractItemView::item:hover
{
    color: rgb(48, 49, 51);
    background-color: #f5f7fa;
}

QComboBox QAbstractItemView::item:selected
{
    color: rgb(48, 49, 51);
    background-color: #f5f7fa;
}

/* QTextEdit */
QTextBrowser#tbLogs
{
    border: 1px solid #d0d0d0;
    border-radius: 0.3em;
    padding: 0.5em;
    color: #444444;
    font: 9pt "Microsoft Yahei";
    selection-color: #fefefe;
    selection-background-color: #cc409eff;
}

/* QLabel */
QLabel#lbLogo
{
    min-width: 6em;
    min-height: 6em;
    max-height: 6em;
    padding: 1em 1em;
    image: url(:/Resource/icon/logo.png);
    border-radius: 0.3em;
    margin-top: 9px;
}

QLabel#lbVersion
{
    color: #ffffff;
    font: 8pt "Ubuntu";
}

QLabel#lbTitle
{
    font: 16pt "Ubuntu";
    color: #fefefe;
}

QLabel#lbMonster
{
    image: url(:/Resource/image/monster.png);
}

/* QScrollBar */
QScrollBar::vertical
{
    background-color: #d0d0d0;
    border: 1px solid #d0d0d0;
    border-radius: 0.2em;
    margin:0px,0px,0px,0px;
    width: 0.4em;
}

QScrollBar::handle:vertical
{
    background-color: #fc9153;
    border: none;
    border-radius: 0.2em;
    min-height: 3em;
    width: 0.4em;
}

QScrollBar::sub-page:vertical {
    background: rgba(0, 0, 0, 6);
}

QScrollBar::add-page:vertical {
    background: rgba(0, 0, 0, 6);
}

QScrollBar::add-line:vertical {
    height: 0em;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
    height: 0em;
    subcontrol-origin:margin;
}

/* QLabel */
QLabel
{
    color: #666666;
}

QLabel[PrefixTitle]
{
    padding: 0em 1em;
}

/* QMenu */
QMenu
{
    min-height: 8.4em;
    background-color: #fefefe;
    border: 1px solid #dcdfe6;
    border-radius: 0.3em;
}

QMenu::item
{
    color: #333333;
    background-color: transparent;
    padding-left: 1em;
    margin: 0.3em 1px;
    min-width: 10em;
    min-height: 1.6em;
}

QMenu::item:selected  {
    background-color: #f2f3f5;
}
